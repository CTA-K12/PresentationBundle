<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta name="application-name" content="{{ mesd_presentation__app_name }}"/>
<meta name="description" content="{{ mesd_presentation__app_description }}" />
<meta name="keywords" content="{{ mesd_presentation__app_keywords }}" />
<link rel="schema.DC" href="http://purl.org/dc/elements/1.1/">
<meta name="DC.title" content="{{ mesd_presentation__app_name }}">
<meta name="DC.identifier" content="{{ mesd_presentation__app_url }}">
<meta name="DC.description" content="{{ mesd_presentation__app_description }}">
<meta name="DC.subject" content="{{ mesd_presentation__app_keywords }}">
<meta name="DC.language" scheme="ISO639-1" content="{{ app.request.locale }}">
<meta name="DC.creator" content="{{ mesd_presentation__org_url }}">
<meta name="DC.contributor" content="{{ mesd_presentation__org_url }}">
<meta name="DC.publisher" content="{{ mesd_presentation__org_url }}">
<meta name="DC.license" content="{{ mesd_presentation__app_license_url_1 }}">
<meta name="DC.license" content="{{ mesd_presentation__app_license_url_2 }}">
<meta name="DC.type" scheme="DCMITYPE" content="http://purl.org/dc/dcmitype/Collection">
<meta name="DC.type" scheme="DCMITYPE" content="http://purl.org/dc/dcmitype/Dataset">
<meta name="DC.type" scheme="DCMITYPE" content="http://purl.org/dc/dcmitype/InteractiveResource">
<meta name="DC.type" scheme="DCMITYPE" content="http://purl.org/dc/dcmitype/Text">
<meta name="DC.relation" content="{{ mesd_presentation__app_url }}">
<link rel="schema.DCTERMS" href="http://purl.org/dc/terms/" />
<meta name="DCTERMS.created" scheme="ISO8601" content="{{ "now"|date("Y-m-d") }}">
<!-- Apple -->
<meta name="apple-mobile-web-app-capable" content="yes" />
<meta name="apple-mobile-web-app-status-bar-style" content="black" />
<!-- Microsoft -->
<meta name="msapplication-tooltip" content="{{ mesd_presentation__app_description }}"/>
<meta name="msapplication-TileColor" content="#e6e6e6"/>
<!-- Google -->
<meta name="google" content="notranslate">
<!-- License and Copyright -->
<meta name="dcterms:rights" content="{{ mesd_presentation__app_license_1 }}">
<meta name="dcterms:rights" content="{{ mesd_presentation__app_license_2 }}">