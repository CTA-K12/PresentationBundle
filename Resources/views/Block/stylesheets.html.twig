{% stylesheets
          '@MesdPresentationBundle/Resources/public/less/theme.less'
          filter='less'
          output='bundles/mesdpresentation/css/mesdpresentation-less.css'
     %}
     <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="all" />
{% endstylesheets %}
{% stylesheets
          '@MesdPresentationBundle/Resources/public/css/jquery/jquery-ui-1.10.2.css'
          '@MesdPresentationBundle/Resources/public/css/jquery/jquery-ui-1.10.3.theme.css'
          '@MesdPresentationBundle/Resources/public/css/*'
          filter='cssrewrite'
          output='bundles/mesdpresentation/css/mesdpresentation-css.css'
     %}
     <link rel="stylesheet" href="{{ asset_url }}" />
{% endstylesheets %}